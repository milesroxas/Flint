{
  "name": "flint",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build-cli.js",
    "build:prod": "tsc && vite build --mode production && webflow extension bundle && mkdir -p bundle/prod/${VITE_BUNDLE_RECIPIENT:-internal} && mv bundle.zip bundle/prod/${VITE_BUNDLE_RECIPIENT:-internal}/v$(node -p \"require('./package.json').version\")_${VITE_BUNDLE_RECIPIENT:-internal}_$(date +'%m-%d_%H-%M-%S').zip",
    "build:dev": "tsc && vite build --mode development && webflow extension bundle && mkdir -p bundle/development/${VITE_BUNDLE_RECIPIENT:-internal} && mv bundle.zip bundle/development/${VITE_BUNDLE_RECIPIENT:-internal}/v$(node -p \"require('./package.json').version\")_${VITE_BUNDLE_RECIPIENT:-internal}_$(date +'%m-%d_%H-%M-%S').zip",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:all": "pnpm typecheck && pnpm lint",
    "format": "biome format --write .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:structure": "vitest run src/features/linter/rules/lumos/structure/__tests__/",
    "test:class-order": "vitest run src/features/linter/rules/lumos/structure/__tests__/class-order.element.test.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@biomejs/biome": "^2.4.4",
    "@clack/prompts": "^1.0.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "@webflow/designer-extension-typings": "^2.0.19",
    "concurrently": "^8.2.2",
    "mini-css-extract-plugin": "^2.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.5",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@posthog/react": "^1.8.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/postcss": "^4.1.11",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "flint": "link:",
    "lucide-react": "^0.536.0",
    "postcss": "^8.5.6",
    "posthog-js": "^1.356.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "zustand": "^5.0.7"
  }
}
